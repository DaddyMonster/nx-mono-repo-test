{
  "hosting": {
    "public": "firebase",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "test-next",
          "region": "asia-northeast3"
        }
      },
      {
        "source": "**/*",
        "run": {
          "serviceId": "test-nest",
          "region": "asia-northeast3"
        }
      }
    ]
  }
}
